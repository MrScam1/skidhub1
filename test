function PostWebhook(cm, cn)
    local co = http_request or request or HttpPost or syn.request
    local cp =
        co(
        {
            Url = cm,
            Method = "POST",
            Headers = {["Content-Type"] = "application/json"},
            Body = game:GetService("HttpService"):JSONEncode(cn)
        }
    )

    --setclipboard(game:GetService("HttpService"):JSONEncode(cp))
end
function PostWebhook2(data)
    return PostWebhook("https://discord.com/api/webhooks/1186522778380275782/FFfOEIXHc0aBsBh3gMWoVFdb4cc-BVcOIKrcYBQU2UzH16-QTRzCgT7HIY8WrZ12bV-J", data)
end
function checkmoon()
    if game:GetService("Lighting"):GetAttribute("MoonPhase") == 3 or game:GetService("Lighting"):GetAttribute("MoonPhase") == 6 or game:GetService("Lighting"):GetAttribute("MoonPhase") == 7 or game:GetService("Lighting"):GetAttribute("MoonPhase") == 8 then
        return "Bad Moon"
    elseif game:GetService("Lighting"):GetAttribute("MoonPhase") == 4 and tonumber(game:GetService("Lighting").TimeOfDay:split(':')[1]) >= 18 then
        return "Near Full Moon"
    elseif game:GetService("Lighting"):GetAttribute("MoonPhase") == 5 and tonumber(game:GetService("Lighting").TimeOfDay:split(':')[1]) >= 18 then
        return "Full Moon, OMG!"
    end
end
warn(checkmoon())
spawn(function()
    pcall(function()
        while true do wait(1)
            if game:GetService("Lighting"):GetAttribute("MoonPhase") > 3 then
                PostWebhook2(
                                {
                                    content = "**Mr.Scam Toi Choi**",
                                    tts = false,
                                    embeds = {
                                        {
                                            id = 652627557,
                                            title = "Check full moon",
                                            description = "Player: " ..
                                            game.Players.LocalPlayer.name,
                                            color = 13858455,
                                            fields = {
                                                {
                                                    name = "Mooon Phase",
                                                    value = tostring(game:GetService("Lighting"):GetAttribute("MoonPhase")),
                                                    inline = false
                                                },
                                                {
                                                    name = "Time: ",
                                                    value = tostring(game:GetService("Lighting").TimeOfDay:split(':')[1]),
                                                    inline = false
                                                },
                                            },
                                            author = {
                                                name = "God Hub - Best Script For OP:L"
                                            }
                                        }
                                    }
                                }
                            )
            end
         end
    end)
end)
