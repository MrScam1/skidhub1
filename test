coroutine.wrap(function()
        spawn(function()
            while wait(0.2) do
                if _G.FastAttack then
                    if n <= 70 then
                        pcall(function()
                            repeat task.wait(0.1)
                                AttackNoCD()
                                n = n +1
                            until n > 70
                        end)
                    else n = 0
                    end
                end
            end
        end)
    end)()
