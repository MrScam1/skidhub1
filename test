																					v5.Humanoid:ChangeState(11);
																					v5.Head.CanCollide = false
																					v5.HumanoidRootPart.Size=Vector3.new(55,55,55);
																					
																				end 
																			end);
																		end 
																	end 
																	if _G.FastAttack then
																		FastAttack = true
																		elseif _G.SuperFastAttack then
																		SFastAttack = true
																	end	
																	topos(v.HumanoidRootPart.CFrame * CFrame.new(0,20,0))
																	AutoHaki()
																	EquipWeapon(_G.SelectWeapon);
																	if (game.Players.LocalPlayer.Character:FindFirstChild("Black Leg") and (game.Players.LocalPlayer.Character:FindFirstChild("Black Leg").Level.Value>=150)) then 
																		game:service("VirtualInputManager"):SendKeyEvent(true,"V",false,game);
																		game:service("VirtualInputManager"):SendKeyEvent(false,"V",false,game);
																	end 
																until not _G.AutoSeaBest or not v.Parent or v.Humanoid.Health <= 0
															end
														end
													end
												end
											elseif game.workspace.Enemies:FindFirstChild("PirateGrandBrigade") then
												for is,vs in pairs(game.workspace.Enemies:GetChildren()) do
													if vs.Name == "PirateGrandBrigade" then
														if v:FindFirstChild("VehicleSeat") then
															repeat wait()
																game.Players.LocalPlayer.Character:WaitForChild("Humanoid").Sit = false
																topos(vs.VehicleSeat.CFrame * CFrame.new(0,15,0))
																EquipAllWeapon()  
																AutoSkill = true
																AimBotSkillPosition = vs.VehicleSeat
																Skillaimbot = true
															until not game:GetService("Workspace").Boats:FindFirstChild("PirateBrigade") or _G.AutoSeaBest == false
															AutoSkill = false
															Skillaimbot = false
														end
													end
												end
											end
										elseif game.workspace.Enemies:FindFirstChild("PirateBrigade") then
											for iss,vss in pairs(game.workspace.Enemies:GetChildren()) do
												if vss.Name == "PirateBrigade" then
													if v:FindFirstChild("VehicleSeat") then
														repeat wait()
															game.Players.LocalPlayer.Character:WaitForChild("Humanoid").Sit = false
															topos(vss.VehicleSeat.CFrame * CFrame.new(0,15,0))
															EquipAllWeapon()  
															AutoSkill = true
															AimBotSkillPosition = vss.VehicleSeat
															Skillaimbot = true
														until not game:GetService("Workspace").Boats:FindFirstChild("PirateBrigade") or _G.AutoSeaBest == false
														AutoSkill = false
														Skillaimbot = false
													end
												end
											end
										end
									elseif game:GetService("Workspace").SeaBeasts:FindFirstChild("SeaBeast1") then  
										for i,v in pairs(game:GetService("Workspace").SeaBeasts:GetChildren()) do
											if v:FindFirstChild("HumanoidRootPart") then
												repeat wait()
													game.Players.LocalPlayer.Character:WaitForChild("Humanoid").Sit = false
													topos(v.HumanoidRootPart.CFrame * CFrame.new(0,500,0))
													
													EquipAllWeapon() 
													
													AutoSkill = true
													AimBotSkillPosition = v.HumanoidRootPart
													Skillaimbot = true
												until not v:FindFirstChild("HumanoidRootPart") or _G.AutoSeaBest == false
												AutoSkill = false
												Skillaimbot = false
											end
										end
									end
							end
						end)
					end
				end)
			end    
		})
	
		
		
	
