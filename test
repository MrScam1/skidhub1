	local Section = M:AddSection({
		Name = "Bone Menu"
	})
	
	local Bone = M:AddLabel("Bone : ")
	
	spawn(function()
		while wait() do
			pcall(function()
				Bone:Set("Bone You Have : "..(game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Bones","Check")))
			end)
		end
	end)
	M:AddToggle({
		Name = "Auto Quest(Bone)",
		Default = _G.questbone,
		Callback = function(Value)
			_G.questbone = Value
		end    
	})
	

	M:AddToggle({
		Name = "Auto Farm Bone",
		Default = _G.Auto_Bone or false,
		Callback = function(Value)
			_G.Auto_Bone = Value
			local boneframe = CFrame.new(-9514, 164, 5787)
			local QuestC = game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest
            local QuestTitle = game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text

			SaveSetting()
			spawn(function()
				while wait() do 
				
					if _G.Auto_Bone and World3 then
						pcall(function()
							repeat task.wait()
								local CFrameMon1111 = nil
							if _G.questbone then
								if  game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Visible == false then
									game:GetService('ReplicatedStorage').Remotes.CommF_:InvokeServer("StartQuest", "HauntedQuest2", 1)
										print("nhan nv")
								elseif game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Visible == true then
									if not string.find(game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, "Demonic Soul") then
										game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("AbandonQuest")
									end
								end
							end
							Sex_Tween = TP1(boneframe)
							Sex_Tween:Play()
							if game:GetService("Workspace").Enemies:FindFirstChild("Reborn Skeleton") or game:GetService("Workspace").Enemies:FindFirstChild("Living Zombie") or game:GetService("Workspace").Enemies:FindFirstChild("Demonic Soul") or game:GetService("Workspace").Enemies:FindFirstChild("Posessed Mummy") then
								for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
									if v.Name == "Reborn Skeleton" or v.Name == "Living Zombie" or v.Name == "Demonic Soul" or v.Name == "Posessed Mummy" then
									if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Humanoid.Health > 0 then
										
												AutoHaki()
												EquipWeapon(_G.SelectWeapon)
												v.HumanoidRootPart.CanCollide = false
												v.Humanoid.WalkSpeed = 0
												v.Head.CanCollide = false 
												StartMagnetBoneMon = true
												if _G.FastAttack then
													FastAttack = true
													elseif _G.SuperFastAttack then
													SFastAttack = true
												end	
												PosMonBone = v.HumanoidRootPart.CFrame
												Sex_Tween = TP1(v.HumanoidRootPart.CFrame * CFrame.new(PosX,PosY,PosZ))
												Sex_Tween:Play()
												
											
										end
									end
								end
							else
								if _G.FastAttack then
									FastAttack = false
									elseif _G.SuperFastAttack then
									SFastAttack = false
								end	
								StartMagnetBoneMon = false
								Sex_Tween = TP1(CFrame.new(-9506.234375, 172.130615234375, 6117.0771484375))
								Sex_Tween:Play()
								for i,v in pairs(game:GetService("ReplicatedStorage"):GetChildren()) do 
									if v.Name == "Reborn Skeleton" or v.Name == "Living Zombie" or v.Name == "Demonic Soul" or v.Name == "Posessed Mummy"  then
										if not CFrameMon1111 then
											pcall(function()
												CFrameMon1111 = CFrame.new(game:GetService("ReplicatedStorage"):FindFirstChild(v.name).WorldPivot.Position) * CFrame.new(0, 15, 0)
											end)
										end
										if not CFrameMon then
											pcall(function()
												CFrameMon1111 = game.Workspace.Enemies:FindFirstChild(v.name):FindFirstChild("HumanoidRootPart").CFrame * CFrame.new(0, 15, 0)
											end)
										end
										if not CFrameMon then
											pcall(function()
												CFrameMon1111 = game.Workspace._WorldOrigin:FindFirstChild("EnemySpawns"):FindFirstChild("Living Zombie [Lv. 2000]" or "Reborn Skeleton [Lv. 1975]" or "Demonic Soul [Lv. 2025]" or "Posessed Mummy [Lv. 2050]").CFrame * CFrame.new(0, 15, 0)
											end)
										end
										Sex_Tween = TP1(CFrameMon1111)
										Sex_Tween:Play()
									end
								end
							end
						until not _G.Auto_Bone or not v.Parent or v.Humanoid.Health <= 0
						end)
					elseif not _G.Auto_Bone and not World3 then
						if game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Visible == true then
							if string.find(game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, "Demonic Soul") then
								game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("AbandonQuest")
							end
						end
					end
				end
			end)        
			
			SaveSetting()
			StopTween(_G.Auto_Bone)
		end    
	})
	
